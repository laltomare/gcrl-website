{
  "name": "gcrl-website",
  "version": "1.0.0",
  "description": "Golden Compasses Research Lodge Website",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "d1:create": "wrangler d1 create gcrl-documents",
    "d1:execute": "wrangler d1 execute gcrl-documents --file=./schema.sql",
    "d1:query": "wrangler d1 execute gcrl-documents --command",
    "migrate-passwords": "npx tsx scripts/set-passwords.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laltomare/gcrl-website.git"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "masonic",
    "lodge"
  ],
  "author": "Golden Compasses Research Lodge",
  "license": "ISC",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251228.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "typescript": "^5.9.3",
    "wrangler": "^4.54.0"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "otpauth": "^9.4.1"
  }
}
